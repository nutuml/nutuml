{
	"name": "nutuml",
	"displayName": "Nutuml",
	"description": "",
	"version": "0.2.1",
	"publisher":"junfengliang",
	"repository": {
		"url": "https://github.com/junfengliang/nutuml/nutuml-vscode"
	},
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:markdown",
		"onCommand:nutuml.helloWorld",
		"onCommand:nutuml.showPreview"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
			  "id": "nutuml",
			  "extensions": [
				".numl",
				".nutuml",
				".nu"
			  ],
			  "aliases": [
				"NutUml"
			  ],
			  "configuration": "./language-configuration.json"
			}
		  ],
		"commands": [
			{
				"command": "nutuml.showPreview",
				"title": "Nutuml Preview",
				"category": "Nutuml",
				"icon": "$(open-preview)"
			},
			{
				"command": "nutuml.helloWorld",
				"title": "Hello World"
			}
		],
		"menus": {
			"editor/title": [
			  {
				"command": "nutuml.showPreview",
				"when": "editorLangId == nutuml && !notebookEditorFocused",
				"group": "navigation"
			  }
			]
		  }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
